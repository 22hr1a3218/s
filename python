def function(x,y,z):
    print('x=',x)
    print('y=',y)
    print('z=',z)
function(10,20,30,)
def function(x,*y,z=100):
    print(x)
    print(y)
    print(z)
function(10,20,30,40,60)
function(10,20,30,40)
function(10,20 ,z=100)
