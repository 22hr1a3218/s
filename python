num=int(input(''))
print('fibonacci seres:')
num1=0
num2=1
count=2
print(num1,end=' ')
print(num2,end=' ')
while count<num:
    result=num1+num2
    num1=num2
    print(result,end=' ')
    num1=num2
    num2=result
    count=count+1
print('\n')
