def decorators_fun(fun):
    def wrapper_fun():
        print('this is added before original code')
        fun()
        print('this is added after original code')
    return wrapper_fun()
@decorators_fun
def fun():
    print('this is original code')
def hi(myname):
    def hello():
        print('***************************')
        myname()
        print('***************************')
    return hello()
@hi
def myname():
    print('my name is G.SHYAM')
print('\n')
