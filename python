def f1(fun,x):
    print('welcome')
    fun(x)
    print('bye')
def f2(x):
    print(x*2)
f1(f2,10)
def f1(fun):
    result = fun(10)
    print(result)
def f2(x):
    return x*2
f1(f2)
print('\n')
def f1(fun):
    result = fun(10)
    print(result)
f2=lambda x:x*2
f1(f2)
print('\n')
def f1(fun):
    result = fun(10)
    print(result)
f1(lambda x:x*2)
